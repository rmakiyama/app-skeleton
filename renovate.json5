{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePresets": [
    ":prHourlyLimit2",
    ":prConcurrentLimit10"
  ],
  "timezone": "Asia/Tokyo",
  "labels": [
    "renovate",
  ],
  "major": {
    "stabilityDays": 14,
  },
  "minor": {
    "stabilityDays": 7,
  },
  "patch": {
    "stabilityDays": 7,
    "automerge": true,
  },
  "branchConcurrentLimit": 3,
}
